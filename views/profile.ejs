<h2>Profile Page</h2>

<h3>Welcome to your PROFILE</h3>
<div class="hidden" id="userMeetings">
  <% userMeetings.forEach(meeting) { %>
    <div id="meetingDate<%= meeting.dataValues.id %>" value="<%= meeting.dataValues.dateTime %>"></div>
    <div id="meetingTitle<%= meeting.dataValues.id %>" value="<%= meeting.dataValues.notes %>"></div>
    <% } %>
</div>

</div>
<!-- insert meetings statistics such as how many meetings they have, how many meetings they have of each categories, etc -->
<div class="ui calendar" id="inline_calendar">
</div>