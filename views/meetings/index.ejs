<h1>Your Meetings
</h1>

<table class="ui sortable celled table">

  <thead>
    <tr>
      <th>Provider</th>
      <th>Date</th>
      <th>Recurring</th>
      <th>Private</th>
      <th>Passcode</th>
      <th>Notes</th>
      <th>URL</th>
    </tr>
  </thead>
  <tbody>
    <%meetings.forEach(meeting=> { %>
      <tr>
        <td>
          <%= meeting.dataValues.provider %>
        </td>
        <td>
          <%= meeting.dataValues.dateTime %>
        </td>
        <td>
          <%= meeting.dataValues.recurring %>
        </td>
        <td>
          <%= meeting.dataValues.private %>
        </td>
        <td>
          <%= meeting.dataValues.passcode %>
        </td>
        <td>
          <%= meeting.dataValues.notes %>
        </td>
        <td>
          <a href="<%=meeting.dataValues.url%>" target="_blank">
            <%= meeting.dataValues.url%>
          </a>
        </td>
      </tr>
      <% }); %>
  </tbody>
  <tfoot>
    <tr>
      <th>Provider</th>
      <th>Date</th>
      <th>Recurring</th>
      <th>Private</th>
      <th>Passcode</th>
      <th>Notes</th>
      <th>URL</th>
    </tr>
  </tfoot>
</table>